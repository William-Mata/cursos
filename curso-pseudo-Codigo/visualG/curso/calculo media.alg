algoritmo "Soma"
var
Num_1, Num_2 , Media: Real
inicio
     Escreva("Escreva aqui o primeiro número da soma: ")
     Leia(Num_1)
     Escreva("Escreva aqui o segundo número da soma: ")
     Leia(Num_2)
     Media <- Num_1 + Num_2 / 2
     Escreva("O Resultado entre ", Num_1, " e " , Num_2, " e igual a: " , Media)
fimalgoritmo
