algoritmo "semnome"
var
   W, N, S: INTEIRO
Funcao SOMA(A,B: INTEIRO) : INTEIRO
Inicio
   RETORNE A + B
FimFuncao
inicio
      Escreva("Digite o primeiro valor: ")
      Leia(W)
      Escreva("Digite o segundo valor: ")
      Leia(N)
      S := SOMA(W, N)
      Escreval("A SOMA ENTRE", W," E",N," É: ", S)
fimalgoritmo
